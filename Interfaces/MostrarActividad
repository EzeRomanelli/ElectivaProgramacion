    public partial class MostrarActividad : Form
    {
        public MostrarActividad(Actividad act)
        {
            InitializeComponent();
            this.label4.Text = act.gsId.ToString();
            this.lblNombreAct.Text = act.gsNombre;
            this.lblDia.Text = act.gsDia;
            this.lblHora.Text = act.gsHora.ToString();
            this.lblLimiteInscriptos.Text = act.gsLimiteInscriptos.ToString();
            this.lblSociosInscriptos.Text = act.gsSociosInscriptos.ToString();
            this.lblProfesor.Text = act.gsProfesor.ToString();
            this.lblPrecio.Text = act.gsPrecio.ToString();
        }

